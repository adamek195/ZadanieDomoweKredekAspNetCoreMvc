@model List<string>

<h1>Cena za wypożyczenie - wybierz z listy</h1>

<!-- linki z grami gdzie można zobaczyć ceny-->
<ul>
    @foreach (var gameName in Model)
    {
    <li>
        <h2> @Html.ActionLink(gameName, "GetGameByName", "Home", new { selectedGame = gameName }, null)</h2>
    </li>
    }
</ul>